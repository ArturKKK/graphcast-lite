{
    "batch_size": 1,
    "learning_rate": 1e-5,
    "num_epochs": 30,
    "wandb_log": true,
    "graph": {
        "grid2mesh_edge_creation": "radius",
        "mesh2grid_edge_creation": "contained",
        "grid2mesh_radius_query": 0.5,
        "mesh_levels": [
            6
        ]
    },
    "pipeline": {
        "encoder": {
            "mlp": {
                "mlp_hidden_dims": [
                    36,
                    72
                ],
                "output_dim": 128,
                "use_layer_norm": true,
                "layer_norm_mode": "node"
            },
            "gcn": {
                "layer_type": "simple_conv"
            }
        },
        "processor": {
            "gcn": {
                "layer_type": "conv_gcn",
                "hidden_dims": [
                    128,
                    128
                ],
                "output_dim": 128,
                "use_layer_norm": true,
                "layer_norm_mode": "node"
            }
        },
        "decoder": {
            "mlp": {
                "mlp_hidden_dims": [
                    72,
                    36
                ],
                "output_dim": 12,
                "use_layer_norm": false
            },
            "gcn": {
                "layer_type": "simple_conv"
            }
        }
    },
    "data": {
        "dataset_name": "64x32_12f_2y_2obs_1pred_uns",
        "num_features_used": 12,
        "obs_window_used": 2,
        "pred_window_used": 1,
        "want_feats_flattened": true
    }
}