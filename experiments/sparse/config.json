        "layer_norm_mode": "node"
      }
    },
    "decoder": {
      "mlp": {
        "mlp_hidden_dims": [
          64,
          64
        ],
        "output_dim": 12,
        "use_layer_norm": false
      },
      "gcn": {
        "layer_type": "simple_conv",
        "hidden_dims": [
          48,
          48
        ],
        "output_dim": 12
      }
    }
  },
  "data": {
    "dataset_name": "64x32_12f_2y_2obs_1pred_uns",
    "num_features_used": 12,
    "obs_window_used": 2,
    "pred_window_used": 1,
    "want_feats_flattened": true
}
